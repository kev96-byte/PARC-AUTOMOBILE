    <script>
        document.addEventListener('DOMContentLoaded', (event) => {
            document.querySelectorAll('.flash-close').forEach(button => {
                button.addEventListener('click', () => {
                    button.parentElement.style.display = 'none';
                });
            });
        });
    </script> 



